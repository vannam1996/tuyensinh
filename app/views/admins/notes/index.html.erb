<div class="row">
  <div class="col-md-12">
    <div class="block full">
      <div class="block-title">
        <div class="block-options pull-right">
          <%= link_to new_admins_note_path, remote: true, title: t("add_note"),
            class: "btn btn-alt btn-sm btn-default" do %>
            <i class="fa fa-plus"></i>
          <% end %>
        </div>
        <h3 class="widget-content-light"><%= t "all" %> <strong><%= t "note" %></strong>
          (<%= DateTime.now.year %> - <%= DateTime.now.year + 1 %>)
        </h3>
      </div>
      <div class="table table-bordered table-striped table-vcenter dataTable no-footer" role="grid">
        <div class="row">
          <div class="col-md-12">
            <table role="grid" class="table table-bordered table-striped table-vcenter dataTable no-footer">
            <thead>
              <tr role="row">
                <th class="text-center"></th>
                <th class="text-center"><%= t "start_time" %></th>
                <th class="text-center"><%= t "end_time" %></th>
                <th class="text-center"><%= t "action" %></th>
              </tr>
            </thead>
            <tbody id="body-table">
              <%= render partial: "table_notes", collection: @notes, as: :note %>
            </tbody>
          </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="modal fade" id="add-note" role="dialog" tabindex="-1" aria-hidden="true"></div>

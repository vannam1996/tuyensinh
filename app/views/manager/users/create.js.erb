<% if @success %>
  $('#add-student').modal('hide');
  $('#table-user').html('<%= j render "table_student" %>');
  alertify.success('<%= @success %>');
<% elsif @error %>
  alertify.error('<%= @error %>');
  alertify.error('<%= j render partial: "shared/error_messages",
    locals: {object: @user} %>');
<% end %>

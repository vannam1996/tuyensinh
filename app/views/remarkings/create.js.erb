<% if @remarking.errors.any? %>
  $('#remarking-new').html('<%= j render "form" %>');
  <% if @error %>
    alertify.error('<%= @error %>');
  <% end %>
<% else %>
  $('#remarking-table').attr("class", "col-md-8");
  $('#timeline').html('<%= j render "timeline_remarkings" %>');

  $('#result-<%= @remarking.result_id %>').attr('class','disable-link');
  $('#text-remaring-20').text('<%= t "sent_remarking" %>');
  $('#remarking-new').modal('hide');
  alertify.success('<%= @success %>');
<% end %>
